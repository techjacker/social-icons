<!-- constant -->
<meta property="fb:app_id" content="{{fbAppId}}">
<meta property="og:site_name" content="{{websiteName}}">

<!-- common but different -->
<meta property="og:url" content="{{url}}">
<meta property="og:title" content="{{pageTitle}}">
<meta property="og:image" content="{{fbImg}}">

{{#graph}}
	<meta property="og:type" content="article">
	<meta property="article:published_time" content="{{publishedTime}}">
	<meta property="article:section" content="visualisations">
{{/graph}}

{{#homepage}}
	<meta property="og:description" content="{{tagline}}">
	<meta property="article:modified_time" content="{{nowtoIsoTime}}">
	<!-- <meta property="article:modified_time" content="2013-09-13T15:11Z"> -->
{{/homepage}}

